    "delay": "0s"
  },
  "filters": {
    "status_codes": [],
    "exclude_status": [404, 400],
    "min_size": 0,
    "max_size": 0,
    "include_regex": "",
    "exclude_regex": "",
    "include_words": [],
    "exclude_words": []
  },
  "headers": {
    "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8"
  },
  "user_agent": "dirsearch-go/2.0",
