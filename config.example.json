{
  "target": "http://example.com",
  "wordlist": "dicc.txt",
  "threads": 20,
  "timeout": "10s",
  "output": {
    "format": "console",
    "file": "",
    "verbose": false,
    "show_errors": false
  },
  "scanner": {
    "methods": ["GET"],
    "extensions": [],
